{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "design-systems": {
      "type": "http",
      "url": "https://design-systems-mcp.southleft.com/mcp"
    },
    "words": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "casual-mcp-server-words"
      ],
      "env": {}
    },
    "urbandictionary": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "urban-dictionary-mcp"
      ],
      "env": {}
    }
  }
}